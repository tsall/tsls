{
  "name": "tsls",
  "version": "0.0.2",
  "description": "",
  "main": "dist/index.js",
  "author": "Hernan Rajchert <hrajchert@gmail.com>",
  "license": "MIT",
  "bin": {
    "tsls": "dist/commands/tsls.js",
    "tsls-build": "dist/commands/tsls-build.js"
  },
  "scripts": {
    "start": "tsns run",
    "test": "run-s build-and:test",
    "tdd": "run-p watch:*",
    "build": "tsns build && chmod a+x dist/commands/tsls*.js",
    "commitmsg": "validate-commit-msg",
    "precommit": "run-s lint:* build",
    "prepush": "run-s build-and:test",
    "lint:ts": "tslint --project tsconfig.json",
    "lint-and-fix:-ts": "tslint --project tsconfig.json --fix",
    "build-and:test": "run-s build",
    "build-and:test2": "run-s build test-once",
    "build-and:start": "run-s build start",
    "watch:ts": "tsc -w",
    "watch:test": "mocha dist/**/*.test.js -w --colors --inspect --growl",
    "test-once": "mocha dist/**/*.test.js --inspect --colors",
    "contributors-add": "all-contributors add",
    "contributors-generate": "all-contributors generate"
  },
  "dependencies": {
    "commander": "2.17.0",
    "moment": "2.22.2",
    "rollup": "0.59.4",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-json": "3.0.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-typescript2": "0.16.1",
    "rxjs": "6.2.2",
    "shelljs": "0.8.2",
    "ts-dynamic-type-checker": "0.0.13"
  },
  "devDependencies": {
    "@types/commander": "2.12.2",
    "@types/jest": "^22.0.0",
    "@types/node": "^7.0.12",
    "@types/shelljs": "0.8.0",
    "all-contributors-cli": "5.4.0",
    "husky": "^0.13.3",
    "lodash.camelcase": "^4.3.0",
    "mocha": "5.2.0",
    "npm-run-all": "^4.0.2",
    "sinon": "^2.1.0",
    "tslint": "^5.1.0",
    "tslint-config-acamica": "^1.0.1",
    "tsns": "0.0.15",
    "typescript": "3.0.1",
    "validate-commit-msg": "^2.12.1"
  }
}
